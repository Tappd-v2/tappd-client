{
   "prettier.enable": true,
   "editor.formatOnSave": true,
   "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
   },
   "tailwindCSS.files.exclude": [
      "**/.git/**",
      "**/node_modules/**",
      "**/.hg/**",
      "**/.svn/**",
      "**/.nuxt/**"
   ],
   "cSpell.words": [
      "cloudinary",
      "Namecheap",
      "Nuxt",
      "spacegrotesk",
      "Tailwindcss"
   ]
}
